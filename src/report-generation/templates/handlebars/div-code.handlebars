<div id="{{fileName}}">
    <table class="array-report method-table">
        <thead class="method-thead">
        <tr>
            <th class="method-name-column">{{fileName}}</th>
            {{#each metricValues}}
                <th class="right bgwhite">
                    {{#if this.isSelected}}
                       <span class="selected" id="">{{this.metricName}}</span>
                    {{else}}
                        <span class="not-selected">{{this.metricName}}</span>
                    {{/if}}
                </th>
                <th class="score">{{this.value}}</th>
            {{/each}}
        </tr>
        </thead>
        <tbody class="method-body">
        <tr class="code-row">
            <td>
                <pre>
                    <code class="language-js">
{{divCode.code}}</code>
                </pre>
            </td>
        </tr>
        </tbody>
    </table>
</div>
